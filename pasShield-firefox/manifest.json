{

    "manifest_version": 2,
    "name": "pasShield",
    "version": "1.0",
    "description": "",
    "homepage_url": "https://github.com/shshengeng/pasShield/tree/main/pasShield-firefox",
    "icons": {
      "48": "image/icons/lock-64.png"
    },

    "background":{
        "scripts": ["js/background.js"]
    },
  
    "browser_action": {
      "default_icon": "image/icons/lock-64.png",
      "default_title": "pasShield",
      "default_popup": "html/unsupported.html"
    },

    "content_scripts": [ {
        "matches": [
          "<all_urls>"
        ],
        "css": [],
        "js": ["js/content.js"],
        "run_at": "document_start"
      } ],

    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "webRequest",
        "webNavigation",
        "webRequestBlocking",
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ]

  }
  